      <Synthetic Name="p">
        <DisplayString>lua 子函数</DisplayString>
        <Expand>
          <CustomListItems>
            <Variable Name="itproto" InitialValue="(Proto**)0"/>
            <Variable Name="i" InitialValue="0"/>

            <Exec>itproto = p</Exec>
            <Loop>
              <Break Condition="i &gt; (sizep - 1)"/>
              <Item Name="{i}">itproto[i]</Item>
              <Exec>i = i + 1</Exec>
            </Loop>
          </CustomListItems>
        </Expand>
      </Synthetic>

